}

.seed-tag-flat-bordered {
    background-image: linear-gradient(164deg,#FFFFF7 0%,#FFF0EB 100%);
    position: relative;
}

.seed-tag-flat-bordered:before {
    content: '';
    transform: scale(0.5);
    transform-origin: 0 0;
    border: 1px solid #f63;
    border-radius: 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    pointer-events: none;
    box-sizing: border-box;
    z-index: 2;
}

