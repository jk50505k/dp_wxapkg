.midas-template-wrapper {
    font-family: PingFangSC-Regular;
    box-sizing: border-box;
    width: 100%;
    padding: 0 0 0 30rpx;
    background: #fff;
}

.midas-template-title {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    display: flex;
    justify-content: space-between;
}

.midas-title {
    color: #111;
    font-size: 30rpx;
}

.midas-tip {
    color: #D5D5D5;
    font-size: 20rpx;
    margin-right: 30rpx;
}

.midas-content {
    width: 100%;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}

.midas-content-item-wrapper {
    width: 100%;
}

.midas-template-content {
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 160rpx;
    margin: 25rpx 0 21rpx 0;
}

.midas-template-content-star-shop {
    display: flex;
    position: relative;
    width: 690rpx;
    height: 250rpx;
    margin: 30rpx 30rpx 20rpx 0;
    font-size: 26rpx;
    color: #777777;
    border: 1rpx solid #e1e1e1;
}

.midas-shop-img {
    width: 160rpx;
    height: 160rpx;
}

.midas-left-content {
    width: 540rpx;
    margin-left: 20rpx;
}

.midas-shop-info {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 41rpx;
    margin-top: 7rpx;
    margin-right: 30rpx;
}

.midas-shop-name {
    flex: 0 1 auto;
    font-size: 34rpx;
    line-height: 40rpx;
    height: 40rpx;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #111111;
}

.midas-shop-distance {
    flex: 0 0 auto;
    font-size: 24rpx;
    color: #777777;
}

.midas-deal-detail {
    margin: 20rpx 28rpx 0 0;
    font-size: 26rpx;
    line-height: 30rpx;
    height: 30rpx;
    color: #666666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-deal-info {
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    margin: 24rpx 30rpx 0 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-price {
    display: flex;
    align-items: baseline;
    line-height: 40rpx;
    flex: 1 1 auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-deal-price-symbol {
    font-size: 26rpx;
    color: #ff6633;
    font-weight: bold;
}

.midas-deal-price {
    font-size: 36rpx;
    color: #ff6633;
    font-weight: bold;
    flex: 0 1 auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-right: 16rpx;
    padding-right: 4rpx;
}

.midas-deal-market-price-symbol {
    font-size: 26rpx;
    color: #777777;
}

.midas-deal-market-price {
    font-size: 26rpx;
    color: #777777;
    text-decoration: line-through;
    flex: 0 1 auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-deal-count {
    font-size: 26rpx;
    color: #777777;
    flex: 0 0 auto;
}

.midas-split-line {
    background: #e1e1e1;
    width: 100%;
    height: 1px;
}

.midas-star-shop-mark {
    width: 74rpx;
    height: 66rpx;
    position: absolute;
    top: -6rpx;
    left: 12rpx;
}

.midas-star-shop-name {
    flex: 0 1 auto;
    font-size: 32rpx;
    line-height: 32rpx;
    height: 32rpx;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #000000;
}

.midas-right {
    display: inline-block;
    width: 400rpx;
    padding: 20rpx;
}

.midas-star-shop-info {
    display: flex;
    flex: 1 1 auto;
    justify-content: space-between;
    align-items: flex-end;
    height: 41rpx;
    margin-top: 7rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-shop-detail {
    display: flex;
    align-items: center;
    flex: 0 1 auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-shop-category {
    flex: 1 1 auto;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.midas-rank-stars {
    flex: 0 0 auto;
    width: 17.6vw;
    height: 3.5vw;
    margin-right: 1.33vw;
    vertical-align: middle;
    background-repeat: no-repeat;
    background-size: 22.933vw auto;
    background-image: url(data:image/png;base64,);
}

.midas-star-50 {
    background-position: 0 -3.8vw;
}

.midas-star-45 {
    background-position: 0 -8.333vw;
}

.midas-star-40 {
    background-position: 0 -12.867vw;
}

.midas-star-35 {
    background-position: 0 -17.4vw;
}

.midas-star-30 {
    background-position: 0 -21.933vw;
}

.midas-star-20 {
    background-position: 0 -26.467vw;
}

.midas-star-10 {
    background-position: 0 -31vw;
}

.midas-star-0 {
    background-position: 0 -35.533vw;
}

.midas-shop-desc {
    height: 74rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin: 14rpx 0 6rpx 0;
    align-self: center;
}

.midas-star-shop-price {
    display: flex;
    align-items: baseline;
    line-height: 44rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.midas-star-deal-price-symbol {
    font-size: 36rpx;
}

.midas-star-deal-price {
    font-size: 44rpx;
    margin-right: 10rpx;
}

.midas-star-shop-count {
    padding: 3rpx 6rpx;
    margin-left: 10rpx;
    border: 1rpx solid #FF6633;
    border-radius: 2rpx;
    color: #FF6633;
    font-size: 22rpx;
    line-height: 22rpx;
    position: relative;
    bottom: 6rpx;
}