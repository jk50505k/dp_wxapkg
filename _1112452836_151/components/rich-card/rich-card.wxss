.media-box {
    overflow: hidden;
    position: relative;
    border-radius: 14rpx;
    box-sizing: border-box;
    flex-wrap: wrap;
    text-align: center;
    display: flex;
}

.media-item {
    display: block;
    position: relative;
    box-sizing: border-box;
    width: 100%;
}

.item-video {
    height: 690rpx;
}

.media-overlay {
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    top: 0;
    background-size: 58rpx 58rpx;
}

.media-img {
    display: block;
    width: 100%;
    padding-bottom: 100%;
    position: relative;
    overflow: hidden;
}

.media-video-preview {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}

.media-video-preview::after {
    content: '';
    position: absolute;
    display: block;
    width: 110rpx;
    height: 110rpx;
    background-image: url(//p0.meituan.net/dpgroup/b1bb733bb9414c0aa7bb0bab733cff581145.png);
    background-size: cover;
    left: 50%;
    top: 50%;
    margin-top: -55rpx;
    margin-left: -55rpx;
}

.video-cover {
    display: block;
    width: 100%;
    height: 100%;
}

.copy-con {
    margin-bottom: 24rpx;
    position: relative;
    overflow: hidden;
}

.copy-con .emoji-img {
    position: relative;
    display: inline-block;
    width: auto;
    height: 36rpx;
    top: -2rpx;
    vertical-align: middle;
}

.reply-con {
    display: inline-block;
}

.reply-con .emoji-img {
    position: relative;
    display: inline-block;
    width: auto;
    height: 36rpx;
    top: -2rpx;
    vertical-align: middle;
}

.c-title {
    color: #111;
    font-size: 30rpx;
    line-height: 48rpx;
    margin-bottom: 6rpx;
}

.c-desc {
    line-height: 48rpx;
    max-height: 288rpx;
    overflow: hidden;
    position: relative;
}

.c-content {
    max-height: 288rpx;
    position: relative;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 240rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 5;
    line-height: 48rpx;
    font-size: 30rpx;
    color: #111;
    background-color: #fff;
    overflow: hidden;
    word-break: break-all;
}

.c-content .c-richtext {
    display: block;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 5;
}

.c-ghost {
    font-size: 30rpx;
}

.c-wrapper {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 6;
    line-height: 48rpx;
    color: #333;
    color: transparent;
    visibility: hidden;
    word-break: break-all;
}

.c-ghost .c-more {
    height: 48rpx;
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    color: #466889;
}

