@import "../../app.wxss";

@import "../../__wuBaseWxss__/1.wxss";

@import "../../__wuBaseWxss__/3.wxss";

.vip {
    background-image: url(data:image/png;base64,);
    width: 40rpx;
    height: 24rpx;
    background-size: cover;
}

.review-item {
    padding: 30rpx 0 0 30rpx;
    background: #fff;
    display: flex;
}

.groupTitle {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    height: 90rpx;
    color: #111;
    padding-right: 30rpx;
    border-bottom: 1rpx solid #e1e1e1;
}

.more-review {
    background-color: #fff;
    padding-left: 30rpx;
}

.down-review-title {
    border-bottom: none;
    position: relative;
}

.down-review-title:before {
    content: "";
    border: solid #e1e1e1;
    border-width: 1px 0 0 0;
    border-radius: 0;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.reviewItem {
    display: flex;
    padding-bottom: 20rpx;
    font-size: 28rpx;
}

.userImg {
    position: relative;
    width: 100rpx;
    height: 100rpx;
}

.userImg .user-vip {
    position: absolute;
    width: 38rpx;
    height: 38rpx;
    right: 0;
    bottom: 0;
}

.userAvatar {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50rpx;
}

.reviewDetail {
    padding: 10rpx 30rpx 20rpx 0;
    margin-left: 20rpx;
    flex: 1;
    overflow: hidden;
    position: relative;
}

.reviewDetail:before {
    content: "";
    border: solid #e1e1e1;
    border-width: 0 0 1px 0;
    border-radius: 0;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.lastReview {
    position: relative;
}

.lastReview:before {
    content: "";
    border: solid #e1e1e1;
    border-width: 0 0 0 0;
    border-radius: 0;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.userName {
    display: flex;
    align-items: center;
    font-size: 32rpx;
    font-weight: bold;
    color: #446889;
    line-height: 32rpx;
    position: relative;
}

.userName .userLevel {
    width: 62rpx;
    height: 28rpx;
    margin-left: 10rpx;
}

.userName .review-other-info {
    position: absolute;
    top: 0;
    right: 20rpx;
}

.userName .review-other-info .honor-pic {
    height: 90rpx;
    width: 90rpx;
    position: absolute;
    top: -10rpx;
    right: 0;
}

.review-lastTime {
    font-size: 22rpx;
    color: #999;
    font-weight: 400;
    padding-top: 10rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.reviewAction {
    padding-top: 10rpx;
    display: flex;
    align-items: center;
}

.actionType,.expense {
    font-size: 24rpx;
    line-height: 24rpx;
    margin-right: 10rpx;
}

.userStar {
    margin-left: 5rpx;
    font-size: 0;
}

.expense {
    margin-left: 5rpx;
}

.reviewText {
    padding-top: 14rpx;
    font-size: 28rpx;
    line-height: 42rpx;
    color: #111;
    word-break: break-all;
    overflow: hidden;
}

.impression {
    margin-right: 30rpx;
    display: flex;
    flex-wrap: wrap;
}

.reviewTruncate {
    display: -webkit-box;
    -webkit-line-clamp: 6;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.twoLine {
    -webkit-line-clamp: 2;
}

.three-line {
    -webkit-line-clamp: 3;
}

.impressionTitle {
    margin-right: 10rpx;
}

.reviewPics {
    width: 100%;
    padding-top: 20rpx;
}

.scrollPics {
    width: 100%;
    height: 160rpx;
    overflow: hidden;
}

._lazyLoadImage {
    position: relative;
}

.img-wrap {
    display: inline-block;
    position: relative;
}

.video-play-icon {
    width: 70rpx;
    height: 70rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
    background-image: url("//p1.meituan.net/dpgroup/182fa939796ed418ed4bd7c88693911e4636.png");
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.riviewPicItem {
    width: 160rpx;
    height: 160rpx;
    margin-right: 10rpx;
    border-radius: 6rpx;
}

.review-title {
    font-size: 32rpx;
}

.userAction {
    padding-top: 20rpx;
    display: flex;
    align-items: center;
    padding-right: 30rpx;
    font-size: 24rpx;
    color: #666;
}

.browser-count {
    font-size: 24rpx;
    margin-top: 20rpx;
    color: #999;
    display: flex;
    justify-content: space-between;
}

.browser-count .flower-browser {
    margin-right: 10rpx;
}

.browser-count .flower-total {
    margin-right: 10rpx;
}

.browser-count .browser-left {
    display: flex;
}

.browser-count .browser-right {
    padding-right: 30rpx;
}

.likeCount {
    flex: 1;
}

.iconLike {
    background-image: url(data:image/png;base64,);
    width: 33rpx;
    height: 31rpx;
    margin-right: 10rpx;
    background-size: cover;
}

.iconLiked {
    background-image: url(data:image/png;base64,);
    width: 32rpx;
    height: 32rpx;
    background-size: cover;
}