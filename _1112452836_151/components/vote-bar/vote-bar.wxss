    padding-left: 30rpx;
    padding-right: 30rpx;
    justify-content: space-between;
    align-items: center;
    background: #FFFFFF;
}

.voteList .voteHeader {
    display: flex;
    flex-direction: row;
    height: 100rpx;
    line-height: 100rpx;
