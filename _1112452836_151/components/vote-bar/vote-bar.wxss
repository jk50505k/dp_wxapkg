.mt20-mb20 {
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.voteList .voteItemLeft {
    display: flex;
    flex: 1;
    overflow: hidden;
}

.voteList .voteItem .voteItemLeft .tagme {
    background: #FFEFEA;
    border-radius: 6rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #FF6633;
    text-align: center;
    height: 42rpx;
    line-height: 42rpx;
    width: 123rpx;
    margin-right: 10rpx;
    padding-left: 9.5rpx;
    padding-right: 9.5rpx;
    align-self: center;
}

.bottom-border {
    position: relative;
}

.bottom-border:before {
    content: '';
    position: absolute;
    width: 89.3%;
    transform: scaleY(0.5);
    transform-origin: 0 0;
    border-bottom: 1px solid #E1E1E1;
    left: 5.3%;
    top: 0;
}

.voteList .voteItem .voteItemLeft .voteTitle {
    font-family: PingFangSC-Medium;
    font-size: 26rpx;
    color: #111111;
    letter-spacing: 0.22px;
    flex: 1;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}

.makeVoteBtnWrap {
    height: 100rpx;
    line-height: 100rpx;
    background: #FFFFFF;
    text-align: center;
    padding-left: 30rpx;
    padding-right: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.makeVoteBtn .voteText {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #111111;
    letter-spacing: 0.22rpx;
}

.makeVoteBtn .voteShopNum {
    font-size: 34rpx;
    color: #ff6633;
    font-weight: 800;
    margin: 0 5rpx;
    white-space: nowrap;
    word-wrap: normal;
    text-overflow: ellipsis;
    overflow: hidden;
    max-width: 70rpx;
    position: relative;
    top: 3rpx;
}

.makeVoteBtnWrap .entry {
    background-image: url(data:image/png;base64,);
    width: 15rpx;
    height: 26rpx;
    background-size: cover;
}

.fixedBar {
    height: 117rpx;
    line-height: 117rpx;
    background: #FFFFFF;
    display: flex;
    flex-direction: row;
    position: fixed;
    z-index: 500;
    width: 100%;
    top: 0;
}

.barWrap {
    display: flex;
    width: 100%;
    padding-left: 30rpx;
    padding-right: 20rpx;
    padding-top: 20rpx;
    flex-direction: row;
    justify-content: space-between;
    box-sizing: border-box;
}

.leftBar {
    display: flex;
    flex-direction: column;
    overflow: hidden;
}

.leftBar .shopName {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #111111;
    letter-spacing: 0.22rpx;
    line-height: 37rpx;
    overflow: hidden;
    text-overflow: ellipsis;
