    max-width: 100%;
}

.leftBar .shopIntro {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #777777;
    letter-spacing: 0.22rpx;
    line-height: 37rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    max-width: 100%;
}

.rightBar {
    background: #FF6633;
    border-radius: 44rpx;
    width: 290rpx;
    height: 50rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    margin-top: 16rpx;
}

.rightBar .addTextBtn {
    font-family: PingFangSC-Regular;
