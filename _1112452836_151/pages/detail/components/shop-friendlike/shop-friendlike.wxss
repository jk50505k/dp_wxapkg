.ad-mode {
    font-size: 20rpx;
    color: #D5D5D5;
}

.lazy-img {
    background-image: url(data:image/jpeg;base64,);
    background-repeat: no-repeat;
    background-position: center;
    width: 100%;
    height: 100%;
    background-size: cover;
}

.picPanel {
    width: 330rpx;
    height: 250rpx;
    position: relative;
}

.friends-title {
    font-size: 38rpx;
    color: #111;
    font-weight: bold;
}

.ad-tag {
    position: absolute;
    bottom: 0;
    font-size: 16rpx;
    color: #fff;
    background-color: rgba(0,0,0,0.4);
    padding: 0 2rpx;
    left: 0;
    height: 20rpx;
    line-height: 22rpx;
}

.spAd-tag {
    bottom: 2rpx;
    left: 1rpx;
    border-radius: 0 0 0 14rpx;
}

.groupItem {
    margin-top: 30rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    font-size: 28rpx;
    background-color: #fff;
}

.groupTitle {
    padding: 30rpx 30rpx 20rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    color: #111;
    border-bottom: 1rpx solid #e1e1e1;
}

.noBorder {
    border: none;
}

.recordList {
    display: flex;
    flex-wrap: wrap;
}

.shopInfo {
    margin-bottom: 20rpx;
    padding: 1rpx;
    position: relative;
}

.shopInfo:before {
    content: '';
    border: solid #e1e1e1;
    border-width: 1px 1px 1px 1px;
    border-radius: 28rpx;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.hasNotPoiShop:nth-of-type(2n) {
    margin-left: 20rpx;
}

.hasPoiShop:nth-of-type(2n+1) {
    margin-left: 20rpx;
}

.hasPoiShop:first-child {
    margin-left: 0;
}

.descPanel {
    width: 310rpx;
    overflow: hidden;
    padding: 20rpx 10rpx;
    display: flex;
    flex-direction: column;
}

.leftPanel {
    position: relative;
    width: 238rpx;
    height: 180rpx;
}

.shopPic {
    width: 330rpx;
    height: 250rpx;
    border-radius: 14rpx 14rpx 0 0;
}

.rightPanel {
    margin-left: 20rpx;
    flex: 1;
    display: flex;
    flex-direction: column;
    color: #777;
    font-size: 26rpx;
    line-height: 26rpx;
    height: 180rpx;
}

.topPanel {
    font-size: 34rpx;
    line-height: 40rpx;
    color: #333;
    font-weight: bold;
    white-space: nowrap;
    word-wrap: normal;
    text-overflow: ellipsis;
    overflow: hidden;
    display: inline-block;
    max-width: 310rpx;
    padding: 8rpx 0;
}

.middlePanel {
    padding: 8rpx 0;
    display: flex;
    align-items: center;
    height: 24rpx;
}

.price {
    color: #666;
    display: inline-block;
    max-width: 200rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 24rpx;
}

.shopStar {
    margin-right: 16rpx;
}

.bottomPanel {
    color: #666;
    font-size: 24rpx;
    padding: 8rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.leftText {
    max-width: 230rpx;
    overflow: hidden;
    height: 30rpx;
    line-height: 30rpx;
}

.region,.category {
    margin-right: 14rpx;
    display: inline-block;
    max-width: 100rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.distance {
    height: 30rpx;
    line-height: 30rpx;
}

.category {
    margin-right: 0;
}

.star {
    display: inline-block;
    width: 132rpx;
    height: 30rpx;
    vertical-align: middle;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 172rpx auto;
}

.star-0 {
    background-position: 0 -260rpx;
}

.star-10 {
    background-position: 0 -226rpx;
}

.star-20 {
    background-position: 0 -193rpx;
}

.star-30 {
    background-position: 0 -159rpx;
}

.star-35 {
    background-position: 0 -126rpx;
}

.star-40 {
    background-position: 0 -93rpx;
}

.star-45 {
    background-position: 0 -59rpx;
}

.star-50 {
    background-position: 0 -26rpx;
}

.starPoiPanel {
    position: relative;
    width: 690rpx;
}

.starPoi {
    display: flex;
}

.starPoi {
    overflow: hidden;
    position: relative;
}

.spPicPanel {
    height: 252rpx;
    position: relative;
}

.spDescPanel {
    padding: 20rpx 20rpx 0 20rpx;
    width: 100%;
}

.spTopPanel {
    max-width: 380rpx;
    padding: 0;
    padding-bottom: 6rpx;
}

.spRegion {
    flex: 1;
}

.text {
    color: #666;
    font-size: 24rpx;
}

.spMiddelPanel {
    display: flex;
    align-items: center;
    justify-content: center;
}

.spRegionText {
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 170rpx;
    vertical-align: top;
}

.sptag {
    margin-top: 18rpx;
    color: #FF6633;
    font-size: 20rpx;
    border: 2rpx solid #FF6633;
    padding: 0 10rpx;
    border-radius: 4rpx;
    display: inline-block;
}

.spIcon {
    position: absolute;
    left: 12rpx;
    top: 5rpx;
    width: 76rpx;
    background-image: url('http://p0.meituan.net/mobilem/ee3d3df4e7ed95b5f97566cf956cd9f39216.png');
}

.spIcon {
    position: absolute;
    left: 12rpx;
    top: -10rpx;
    width: 57rpx;
    height: 53rpx;
    background-size: contain;
    background-repeat: no-repeat;
    background-image: url('http://p0.meituan.net/mobilem/ee3d3df4e7ed95b5f97566cf956cd9f39216.png');
    z-index: 10;
}

.reviewPanel {
    font-size: 24rpx;
    color: #666;
    position: relative;
    text-indent: 24rpx;
    margin-top: 18rpx;
}

.reviewPaneltext {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.reviewPanel:before {
    content: '';
    width: 15rpx;
    height: 13rpx;
    background: url('https://www.dpfile.com/picasso/images/445f4e20afb21567927dd9eecf670b7c.png') no-repeat;
    background-size: 15rpx 13rpx;
    display: inline-block;
    position: absolute;
    top: 6rpx;
}

.reviewPanel:after {
    content: '';
    width: 15rpx;
    height: 13rpx;
    background: url('https://www.dpfile.com/picasso/images/445f4e20afb21567927dd9eecf670b7c.png') no-repeat;
    background-size: 15rpx 13rpx;
    display: inline-block;
    position: absolute;
    transform: rotate(180deg);
    bottom: 16rpx;
    right: 5rpx;
}