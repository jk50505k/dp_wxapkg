.dineVote {
    position: fixed;
    bottom: 145rpx;
    right: 30rpx;
    font-weight: 500;
    z-index: 9;
    border: solid 1rpx #e1e1e1;
    border-radius: 50rpx;
    background: #fff;
    box-shadow: 0px 2px 8px 0px #e1e1e1;
}

.quickCover {
    width: 96rpx;
    height: 96rpx;
    color: #111;
    font-size: 22rpx;
    text-align: center;
    z-index: 30;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    transition: all 0.2s;
    border-radius: 50%;
    overflow: hidden;
}

.icon {
    position: relative;
    top: 2rpx;
    width: 110rpx;
    height: 110rpx;
}

.icon-wrap {
    height: 100%;
    width: 100%;
    vertical-align: middle;
    display: flex;
    align-items: center;
    justify-content: center;
}

.cover-txt {
    display: flex;
    height: 100%;
    width: 100%;
    vertical-align: middle;
    justify-content: center;
    align-items: center;
    padding: 20rpx;
    transition: opacity 1s;
    opacity: 1;
}