.Hide {
    display: none;
}

.info-ktvbooking {
    background: #fff;
    margin-top: 30rpx;
}

.info-ktvbooking .bottom-line {
    background-image: linear-gradient(0,#e1e1e1,#e1e1e1 50%,transparent 0);
    background-position: 100% 100%;
    background-size: 100% 1px;
    background-repeat: no-repeat;
}

.info-ktvbooking .empty-timepanel {
    height: 90rpx;
    display: flex;
    align-items: center;
    overflow-x: auto;
    justify-content: center;
}

.info-ktvbooking .empty-timepanel .kb-tp-empty {
    text-align: center;
    line-height: 90rpx;
    font-size: 26rpx;
    color: #999;
}

.info-ktvbooking .ktv-bktitle {
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 30rpx;
    position: relative;
    font-size: 28rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #e1e1e1;
}

.info-ktvbooking .ktv-bktitle .book-txt {
    font-size: 28rpx;
}

.info-ktvbooking .ktv-bktitle .mark-book {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    margin-left: 0rpx;
    margin-right: 10rpx;
}

.info-ktvbooking .ktv-bktitle .ktv-bktitle-tag {
    display: inline-block;
    font-size: 18rpx;
    line-height: 12rpx;
    padding: 0 3rpx;
    border: 1rpx solid #FF946B;
    color: #f63;
    border-radius: 2rpx;
}

.info-ktvbooking .ktv-bktitle .ktv-bktitle-num {
    position: absolute;
    right: 30rpx;
    color: #777;
    font-size: 26rpx;
}

.info-ktvbooking .ktv-bk-daytab {
    height: 90rpx;
}

.info-ktvbooking .ktv-bk-daytab .mb-nav-scroll-wrapper {
    height: 90rpx;
    position: static;
    overflow-x: auto;
}

.info-ktvbooking .ktv-bk-daytab .nav-tab-scroll {
    height: 90rpx;
}

.info-ktvbooking .ktv-bk-daytab .nav-tab-scroll .item {
    height: 90rpx;
    width: 124rpx;
}

.info-ktvbooking .ktv-bk-daytab .nav-tab-scroll .item .kb-dt-tag {
    position: absolute;
    left: 50rpx;
    top: 7rpx;
    height: 12rpx;
    line-height: 12rpx;
    border-radius: 6rpx;
    font-size: 18rpx;
    color: #FFF;
    padding: 0 4rpx;
    background-color: #FF6525;
}

.info-ktvbooking .ktv-bk-daytab .nav-tab-scroll .item .main {
    line-height: 36rpx;
}

.info-ktvbooking .ktv-bk-daytab .nav-tab-scroll .item .sub {
    color: #333;
    line-height: 26rpx;
}

.info-ktvbooking .ktv-bk-daytab .nav-tab-scroll .item.on .sub {
    color: #f63;
}

.info-ktvbooking .ktv-bk-timepanel {
    height: 90rpx;
    display: flex;
    align-items: center;
    overflow-x: auto;
    padding-left: 30rpx;
}

.info-ktvbooking .ktv-bk-timepanel::after {
    content: '';
    display: block;
    height: 0;
    clear: both;
}

.info-ktvbooking .ktv-bk-timepanel .mb-select-panel {
    white-space: nowrap;
    float: left;
    display: flex;
    flex-direction: row;
}

.info-ktvbooking .ktv-bk-timepanel .mb-select-panel .mb-item-contrain-3 {
    width: auto;
    float: none;
    box-sizing: border-box;
    flex: 0 0 auto;
    display: flex;
    flex-direction: column;
    margin-right: 10rpx;
}

.info-ktvbooking .ktv-bk-timepanel .mb-select-panel .mb-item-contrain-3 .mb-item {
    height: 50rpx;
    line-height: 50rpx;
    padding: 0 10rpx;
}

.info-ktvbooking .ktv-bktlist .ktv-bklist-title {
    position: relative;
    padding-left: 50rpx;
    font-size: 24rpx;
    color: #666;
    height: 70rpx;
    line-height: 70rpx;
}

.info-ktvbooking .ktv-bktlist .ktv-bklist-title::before {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 4rpx;
    content: ' ';
    display: block;
    background: #E6E6E6;
    background: linear-gradient(#e6e6e6,#f8f8f8);
}

.info-ktvbooking .ktv-bktlist .ktv-bklist-title::after {
    content: ' ';
    display: block;
    position: absolute;
    background: #f63;
    width: 10rpx;
    height: 10rpx;
    border-radius: 100%;
    left: 24rpx;
    top: 30rpx;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list {
    position: relative;
    padding: 10rpx 0;
    padding-left: 30rpx;
    border-bottom: 1rpx solid #e1e1e1;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list:last-child {
    border: none;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list::after {
    content: '';
    display: block;
    height: 0;
    clear: both;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-con {
    margin-right: 200rpx;
    display: flex;
    flex-direction: column;
    min-height: 80rpx;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-con .kbl-t1 {
    line-height: 44rpx;
    font-size: 30rpx;
    color: #333;
    font-weight: 400;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-con .kbl-t2 {
    line-height: 38rpx;
    font-size: 24rpx;
    color: #999;
    font-weight: 400;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-price {
    position: absolute;
    height: 60rpx;
    top: 50%;
    right: 170rpx;
    margin-top: -28rpx;
    text-align: right;
    flex: 0 0 auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
    font-size: 26rpx;
    font-weight: 700;
    color: #f63;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-price .kbl-price-i {
    line-height: 36rpx;
    flex: 0 0 auto;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-price .kbl-p-yan {
    font-weight: 400;
    font-size: 24rpx;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-price sub {
    font-weight: 400;
    font-size: 24rpx;
    color: #999;
    vertical-align: inherit;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-price .kbl-p-tag {
    display: inline-block;
    font-size: 18rpx;
    font-weight: 400;
    line-height: 12rpx;
    padding: 0 3rpx;
    border: 1rpx solid #FF946B;
    color: #f63;
    border-radius: 2rpx;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-btn {
    position: absolute;
    right: 30rpx;
    top: 50%;
    margin-top: -24rpx;
    display: block;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list .kbl-btn:visited {
    color: #FFF;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list.ktv-bkl-saleout .kbl-con .kbl-t1,.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list.ktv-bkl-saleout .kbl-price {
    color: #999;
}

.info-ktvbooking .ktv-bktlist .ktv-bkl-wrap .ktv-bkl-list.ktv-bkl-saleout .kbl-price .kbl-p-tag {
    border: 1rpx solid #aaa;
    color: #999;
}

.info-ktvbooking .ktv-bktlist .kbl-showmore {
    display: block;
    line-height: 38rpx;
    text-align: center;
}

.info-ktvbooking .ktv-bktlist .kbl-showmore .mb-arrow {
    position: relative;
    display: inline-block;
    margin-left: 5rpx;
    margin-bottom: 2rpx;
    border: 2rpx solid #bbb;
    width: 8rpx;
    height: 8rpx;
    border-top: none;
    border-left: none;
    transform: rotate(45deg);
}

.disable-btn {
    background-color: #ccc!important;
    border: 0!important;
    color: #fff;
}

.primary-btn-sm {
    min-width: 110rpx;
    font-family: inherit;
    text-align: center;
    box-sizing: border-box;
    display: inline-block;
    padding: 0 10rpx;
    border: 0;
    background-color: #f63;
    color: #fff;
    font-size: 26rpx;
    border-radius: 4rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.mb-btn-container {
    display: flex;
    flex-flow: row nowrap;
    padding: 0 10rpx;
}

.mb-btn-container a,.mb-btn-container button {
    flex: 1;
    display: block;
    width: 1%;
    margin-left: 5rpx;
    margin-right: 5rpx;
}

.mb-select-panel {
    font-size: 24rpx;
}

.mb-select-panel:after {
    content: " ";
    clear: both;
    display: block;
    height: 0;
}

.mb-select-panel .mb-item {
    text-align: center;
    border-radius: 2rpx;
    border: 2rpx solid #e1e1e1;
    box-sizing: border-box;
}

.mb-select-panel .mb-selected {
    border: 2rpx solid #f63;
    background: #fff1ed;
}

.mb-select-panel .mb-item-contrain-3 {
    padding: 5rpx;
    float: left;
}

.mb-select-panel .mb-item-contrain-3 .mb-item {
    line-height: 40rpx;
    height: 40rpx;
}

.mb-nav-scroll-wrapper {
    width: 100%;
    overflow: hidden;
    position: absolute;
    height: 45rpx;
}

.nav-tab-scroll {
    background-color: #fff;
    height: 45rpx;
    white-space: nowrap;
    min-width: 100%;
    float: left;
    display: flex;
    flex-direction: row;
}

.nav-tab-scroll .item {
    text-align: center;
    height: 45rpx;
    position: relative;
    box-sizing: border-box;
    flex: 0 0 auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.nav-tab-scroll .item .main {
    font-size: 26rpx;
    display: block;
    color: #333;
    padding: 0 30rpx;
    line-height: 30rpx;
    flex: 0 0 auto;
}

.nav-tab-scroll .item .sub {
    font-size: 24rpx;
    display: block;
    color: #999;
    padding: 0 30rpx;
    line-height: 14rpx;
    flex: 0 0 auto;
}

.nav-tab-scroll .item.on:after {
    content: '';
    display: block;
    width: 100%;
    height: 2rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: #f63;
}

.nav-tab-scroll .item.on .main,.nav-tab-scroll .item.on .sub {
    color: #f63;
}

.booking-icon {
    width: 32rpx;
    height: 32rpx;
    margin-left: 10rpx;
    background: url(https://www.dpfile.com/sc/mobile/mapi-dp-web/search_list_book.png);
    background-size: cover;
}

.iconOpen {
    width: 16rpx;
    height: 16rpx;
    margin: -10rpx 0 0 10rpx;
    border-right: 4rpx solid #bbb;
    border-bottom: 4rpx solid #bbb;
    transform: rotate(45deg);
}

.toMore {
    height: 90rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}