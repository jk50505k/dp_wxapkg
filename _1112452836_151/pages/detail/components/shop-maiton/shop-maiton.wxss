.hh {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -moz-box-orient: horizontal;
    flex-direction: row;
}

.vv {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -moz-box-orient: vertical;
    flex-direction: column;
}

.c-c {
    -moz-box-align: center;
    -webkit-box-align: center;
    align-items: center;
    -moz-box-pack: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.l-c {
    -moz-box-align: center;
    -webkit-box-align: center;
    align-items: center;
    -moz-box-pack: flex-start;
    -webkit-box-pack: flex-start;
    justify-content: flex-start;
}

.r-c {
    -moz-box-align: center;
    -webkit-box-align: center;
    align-items: center;
    -moz-box-pack: flex-end;
    -webkit-box-pack: flex-end;
    justify-content: flex-end;
}

.content {
    background: #fff;
    padding: 20rpx 30rpx;
}

.content .up {
    height: 70rpx;
    position: relative;
}

.up .icon {
    width: 40rpx;
    height: 40rpx;
}

.up .icon image {
    width: 100%;
    height: 100%;
    display: block;
}

.up .desc {
    margin-left: 20rpx;
    font-size: 28rpx;
    color: #333;
    margin-top: 2rpx;
    height: 40rpx;
    font-size: 32rpx;
}

.up .btn {
    position: absolute;
    right: 0;
    width: 120rpx;
    height: 50rpx;
    border-radius: 100rpx;
    background: #ff6632;
    color: #fff;
    font-size: 28rpx;
}

.tip-img {
    width: 190rpx;
    height: 28rpx;
    position: absolute;
    right: 134rpx;
}

.downf {
    position: relative;
}

.downf .down {
    margin-top: 20rpx;
    width: 600rpx;
}

.downf .down:first-child {
    margin-top: 0rpx;
}

.downf .down .dicount {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #111111;
    margin-left: 62rpx;
    max-width: 206rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.downf .down:first-child .dicount {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #FF6633;
}

.downf .down .desc {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #777777;
    margin-left: 20rpx;
    max-width: 260rpx;
}

.downf .sell {
    position: absolute;
    right: 6rpx;
    top: 3rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #777777;
}

.min-d {
    min-height: 40rpx;
}

.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}