@import "../../../../__wuBaseWxss__/4.wxss";

.lazy-img {
    background-image: url(data:image/jpeg;base64,);
    background-repeat: no-repeat;
    background-position: center;
    width: 100%;
    height: 100%;
    background-size: cover;
}

.tuanInfo {
    display: flex;
    flex-direction: column;
    margin-top: 20rpx;
    background-color: #fff;
}

.tuanMain {
    display: flex;
}

.toMore {
    height: 90rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.border-top {
    position: relative;
}

.border-top:before {
    content: '';
    border: solid #e1e1e1;
    border-width: 1px 0 0 0;
    border-radius: 0;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.iconOpen {
    width: 16rpx;
    height: 16rpx;
    margin: -10rpx 0 0 10rpx;
    border-right: 4rpx solid #bbb;
    border-bottom: 4rpx solid #bbb;
    transform: rotate(45deg);
}

.iconClose {
    width: 16rpx;
    height: 16rpx;
    margin: 10rpx 0 0 10rpx;
    border-right: 4rpx solid #bbb;
    border-bottom: 4rpx solid #bbb;
    transform: rotate(225deg);
}

.detail-icon {
    margin: 20rpx 20rpx 0 30rpx;
    width: 40rpx;
    height: 40rpx;
}

.tuanList {
    flex: 1;
    padding-top: 10rpx;
}

.tuanItem {
    display: flex;
}

.tuanItem.Hide {
    display: none;
}

.tuanItem:last-child .item-body::before {
    border: none;
}

.item-body {
    display: flex;
    align-items: center;
    flex: 1;
    padding: 20rpx 30rpx 18rpx 0;
    position: relative;
}

.item-body:before {
    content: '';
    border: solid #e1e1e1;
    border-width: 0 0 1px 0;
    border-radius: 0;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.tagicon {
    position: absolute;
    top: 30rpx;
    left: 0;
    height: 28rpx;
    max-width: 100rpx;
}

.tuanPic {
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
    align-self: flex-start;
    border-radius: 10rpx;
}

.tuanRightPanel {
    flex: 1;
    position: relative;
}

.tuanDesc {
    flex: 1;
    display: flex;
}

.sold {
    font-size: 26rpx;
    color: #777;
    align-self: flex-end;
}

.tuanTitle {
    flex: 1;
    color: #111;
    font-size: 30rpx;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    line-height: 34rpx;
    margin-bottom: 10rpx;
    max-width: 360rpx;
}

.tuanSubtitle {
    display: flex;
    align-items: flex-end;
    position: relative;
}

.tuanPrice {
    color: #f63;
    font-size: 40rpx;
    margin-right: 10rpx;
    line-height: 40rpx;
}

.lineThrough {
    text-decoration: line-through;
}

.tuanPriceOri {
    color: #777;
    font-size: 26rpx;
    line-height: 28rpx;
}

.discount {
    margin-left: 5rpx;
    font-size: 18rpx;
    color: #f63;
    border: 1px solid #f63;
    height: 36rpx;
    padding: 2rpx 4rpx;
    border-radius: 4rpx;
    box-sizing: border-box;
}

.tuanSale {
    position: absolute;
    display: flex;
    flex-direction: column;
    justify-content: center;
    top: 0;
    right: 0;
    height: 104rpx;
    width: 120rpx;
}

.pintuanSale {
    justify-content: space-between;
}

.pt-text {
    text-align: center;
    font-size: 28rpx;
    color: #fff;
    background: #f63;
    width: 120rpx;
    border-radius: 100rpx;
    height: 50rpx;
    line-height: 50rpx;
    align-self: flex-end;
}

.reduce-label {
    display: block;
    margin-left: 10rpx;
    height: 38rpx;
    line-height: 38rpx;
    box-sizing: border-box;
    font-size: 22rpx;
    color: #ff6633;
    border: 2rpx solid #ff6633;
    border-radius: 2px;
    text-align: center;
    padding: 0 12rpx;
    max-width: 244rpx;
    overflow: hidden;
    white-space: nowrap;
    word-wrap: normal;
    text-overflow: ellipsis;
}

.cut-down-bnt {
    height: 40rpx;
    line-height: 40rpx;
    font-size: 20rpx;
    padding: 0 2rpx;
    margin-left: 20rpx;
    color: #f63;
    border: 1rpx solid #f63;
    background: #fff;
}

.cut-down-bnt:after {
    content: "";
    border: none;
}

.zongPintuan {
    font-size: 24rpx;
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.zongpticon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
    background: url(data:image/png;base64,);
    background-size: cover;
}

.zongpttext {
    color: #FF6633;
}