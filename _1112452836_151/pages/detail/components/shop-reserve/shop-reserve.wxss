.play-reserve-box {
    background: #fff;
}

.icon-serve {
    width: 24rpx;
    height: 24rpx;
    background-size: cover;
    vertical-align: middle;
    margin-right: 10rpx;
}

.play-reserve-top {
    padding: 30rpx 24rpx;
    border-bottom: 1rpx solid #dfdfdf;
    overflow: hidden;
    display: flex;
    align-items: center;
}

.item-right-flex-1 {
    flex: 1;
    text-align: right;
}

.joy-icon-tip-box {
    margin-left: 30rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.joy-icon-tip-item {
    display: inline-block;
    margin-right: 20rpx;
}

.joy-icon-tip-item:last-child {
    margin-right: 0;
}

.flex-box {
    display: flex;
}

.reserve-tcl {
    margin-top: 5rpx;
    align-self: flex-start;
}

.booking-icon,.play-reserve-title,.icon-serve,.play-text {
    display: inline-block;
    vertical-align: middle;
}

.play-reserve-title {
    margin-left: 20rpx;
}

.booking-icon {
    width: 40rpx;
    height: 40rpx;
    margin-left: 10rpx;
    background-size: cover;
}

.play-reserve-title {
    font-size: 30rpx;
    color: #333333;
}

.play-text {
    font-size: 24rpx;
    color: #999999;
}

.play-text-em {
    color: #5EB22D;
    display: inline-block;
}

.toMore {
    height: 90rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.border-top {
}

.iconOpen {
    width: 16rpx;
    height: 16rpx;
    margin: -10rpx 0 0 10rpx;
    border-right: 4rpx solid #bbb;
    border-bottom: 4rpx solid #bbb;
    transform: rotate(45deg);
}

.iconClose {
    width: 16rpx;
    height: 16rpx;
    margin: 10rpx 0 0 10rpx;
    border-right: 4rpx solid #bbb;
    border-bottom: 4rpx solid #bbb;
    transform: rotate(225deg);
}

.play-reserve-list {
    padding-left: 30rpx;
}

.play-reserve-item {
    border-bottom: 1rpx solid #dfdfdf;
    display: flex;
    padding-top: 20rpx;
    padding-bottom: 14rpx;
    padding-right: 30rpx;
}

.no-border {
    border-bottom: none;
}

.reserve-btn {
    width: 120rpx;
    height: 72rpx;
    background: #FF6633;
    border-radius: 4rpx;
    color: #fff;
    text-align: center;
    line-height: 72rpx;
    font-size: 32rpx;
    margin-left: 10rpx;
    display: inline-block;
}

.icon-fire {
    width: 24rpx;
    height: 30rpx;
    display: block;
    vertical-align: middle;
    background-size: cover;
    margin-right: 10rpx;
}

.sku-title {
    max-width: 420rpx;
    font-size: 30rpx;
    color: #333333;
    display: block;
    vertical-align: middle;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.row-item {
    margin-bottom: 2rpx;
}

.bath-booking-row {
    margin-top: 6rpx;
}

.bath-time,.bath-cate,.bath-tags {
    font-size: 22rpx;
    color: #999999;
    display: inline-block;
}

.bath-tags:before {
    content: "|";
    font-size: 22rpx;
    color: #D7D7D7;
    margin: 0 12rpx;
}

.bath-tag {
    margin-right: 5rpx;
    display: inline-block;
}

.bath-booking-text {
    font-size: 24rpx;
    display: inline-block;
    margin-right: 10rpx;
}

.sku-price {
    font-size: 32rpx;
    color: #FC5832;
    display: block;
    text-align: right;
}

.sku-price .sku-price-after {
    display: inline-block;
    vertical-align: baseline;
    font-size: 24rpx;
}

.sku-price.middle {
    margin-top: 16rpx;
}

.sku-price:before {
    content: "￥";
    vertical-align: baseline;
    font-size: 24rpx;
    color: #FC5832;
}

.sku-origin-price {
    line-height: 36rpx;
    text-align: right;
}

.sku-origin-before {
    color: #999999;
    font-size: 20rpx;
    text-decoration: none;
    display: inline-block;
}

.sku-origin-price-text {
    font-size: 24rpx;
    color: #999999;
    text-decoration: line-through;
    display: inline-block;
}

.flex-1 {
    flex: 1;
}

.new-reduce {
    text-align: right;
}

.new-reduce-tag {
    background: #FFFFFF;
    border: 1rpx solid #FF9470;
    border-radius: 3rpx;
    font-size: 20rpx;
    color: #FF6633;
    padding: 0 8rpx;
    height: 28rpx;
    line-height: 28rpx;
    display: inline-block;
}

.align-center {
    align-items: center;
}

.has-ordered {
    font-size: 22rpx;
    color: #999999;
    text-align: right;
    align-self: flex-end;
    margin-top: 17rpx;
    padding-right: 4rpx;
    width: 120rpx;
    overflow: hidden;
    white-space: nowrap;
}

.ordered-count {
    display: inline-block;
}

.reserve-time-wrapper {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.65);
    z-index: 100;
}

.reserve-time-box {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
}

.reserve-select-top {
    position: relative;
    height: 80rpx;
    line-height: 80rpx;
}

.reserve-select-title {
    text-align: center;
    font-size: 28rpx;
    color: #999999;
    vertical-align: middle;
}

.reserve-select-title:after,.reserve-select-title:before {
    content: "";
    display: inline-block;
    width: 110rpx;
    height: 1rpx;
    vertical-align: middle;
    background: #D7D7D7;
}

.reserve-select-title:after {
    margin-left: 20rpx;
}

.reserve-select-title:before {
    margin-right: 20rpx;
}

.reserve-select-close {
    width: 64rpx;
    height: 64rpx;
    background-image: url(data:image/png;base64,);
    background-size: cover;
    position: absolute;
    top: 0;
    right: 0;
}

.week-list {
    height: 100rpx;
    line-height: 100rpx;
    width: 100%;
    overflow-x: scroll;
    overflow-y: hidden;
    -webkit-overflow-scrolling: touch;
    padding: 0 26rpx;
    white-space: nowrap;
    position: relative;
    border-bottom: 1rpx solid #D7D7D7;
}

.week-list:after {
}

.week-item {
    white-space: nowrap;
    display: inline-block;
    margin-right: 40rpx;
    box-sizing: border-box;
    height: 100rpx;
}

.week-item.active {
    border-bottom: 4rpx solid #FF6633;
}

.week-item.active .week-title {
    color: #FF6633;
}

.week-item.active .week-day {
    color: #FF6633;
}

.week-item view {
    display: inline-block;
}

.week-title {
    color: #333333;
    font-size: 30rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.week-day {
    font-size: 28rpx;
    color: #333333;
    vertical-align: middle;
}

.week-promo-icon {
    width: 28rpx;
    height: 28rpx;
    background-size: cover;
    vertical-align: middle;
    margin-left: 5rpx;
}

.halfday-ctl {
    height: 100rpx;
    position: relative;
    border-bottom: 1rpx solid #D7D7D7;
}

.halfday-ctl:after {
}

.halfday-item {
    width: 50%;
    height: 100%;
    display: inline-block;
    text-align: center;
    position: relative;
}

.halfday-tab-bottom {
    width: 100%;
    height: 14rpx;
    display: none;
    background-size: cover;
    position: absolute;
    bottom: -1rpx;
    left: 0;
    z-index: 11;
}

.halfday-tab-bottom.active {
    display: inline-block;
}

.halfday-tab-bottom.half-0 {
    background-image: url(data:image/png;base64,);
}

.halfday-tab-bottom.half-1 {
    background-image: url(data:image/png;base64,);
}

.halfday-item:first-child:after {
    content: "";
    width: 1rpx;
    height: 40rpx;
    background: #e1e1e1;
    vertical-align: middle;
    display: inline-block;
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
}

.halfday-icon {
    width: 44rpx;
    height: 44rpx;
    background-size: cover;
    display: inline-block;
    margin-right: 15rpx;
    vertical-align: middle;
}

.halfday-text {
    display: inline-block;
    vertical-align: middle;
    line-height: 100rpx;
}

.halfday-item.active .halfday-text {
    color: #FF6633;
}

.active.halfday-item:last-child .halfday-text {
    color: #4263A6;
}

.time-price-box {
    padding-left: 20rpx;
    max-height: 500rpx;
    overflow: hidden;
}

.time-price-box.empty-box {
    height: 400rpx;
}

.time-empty-msg {
    font-size: 28rpx;
    color: #999999;
    margin-top: 84rpx;
    text-align: center;
}

.time-empty-icon {
    width: 72rpx;
    height: 72rpx;
    background-size: cover;
    margin: 46rpx auto 0;
}

.time-price-item {
    display: inline-block;
    width: 160rpx;
    height: 120rpx;
    border: 2rpx solid #E1E1E1;
    border-radius: 6rpx;
    font-size: 24rpx;
    color: #666666;
    margin-top: 20rpx;
    text-align: center;
    position: relative;
    margin-right: 20rpx;
}

.padding-20 {
    width: 100%;
    height: 20rpx;
    content: '';
    display: block;
}

.time-price-item:nth-child(4n) {
    margin-right: 0;
}

.time-price-item.active {
    border: 2rpx solid #FFC2AD;
    color: #FF6633;
    background: #fff8f6;
}

.time-price-item.active .display-time {
    color: #FF6633;
}

.display-time {
    font-size: 32rpx;
    color: #111111;
    line-height: 32rpx;
    margin-top: 17rpx;
}

.display-time.full {
    color: #999999;
}

.display-price.full {
    color: #999999;
}

.display-price {
    margin-top: 8rpx;
}

.display-stock-desc {
    background: #999999;
    border: 2rpx solid #999999;
    border-radius: 0 0 3rpx 3rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
    font-size: 20rpx;
    color: #FFFFFF;
    line-height: 28rpx;
    box-sizing: border-box;
    height: 30rpx;
}

.display-stock-desc.warn {
    color: #FF6633;
    background: #FFC2AD;
    border: 2px solid #FFC2AD;
}

.display-next-day {
    background: #355FB3;
    border-radius: 4rpx 0 4rpx 0;
    width: 43rpx;
    height: 24rpx;
    line-height: 28rpx;
    font-size: 20rpx;
    color: #FFFFFF;
    text-align: center;
    position: absolute;
    top: 0;
    left: 0;
}

.display-price-promo {
    position: absolute;
    right: 0;
    top: 0;
    width: 28rpx;
    height: 28rpx;
    background-size: cover;
    vertical-align: middle;
}

.reserve-sku-price,.reserve-sku-max-price {
    font-size: 30rpx;
    color: #FC5832;
    line-height: 24rpx;
    display: inline-block;
}

.reserve-sku-price:before {
    font-size: 24rpx;
    color: #FC5832;
    content: "￥";
}

.reserve-sku-max-price:before {
    content: '-';
    display: inline-block;
}

.reserve-time-info {
    font-size: 24rpx;
    color: #999999;
    margin-bottom: 32rpx;
    margin-top: 10rpx;
}

.reserve-time-day {
    margin-right: 20rpx;
}

.reserve-time-hour,.reserve-time-duration,.reserve-time-day {
    display: inline-block;
}

.reserve-time-hour {
    margin-right: 15rpx;
}

.reserve-time-duration:before {
    content: "";
    width: 2rpx;
    height: 20rpx;
    line-height: 40rpx;
    background: #D7D7D7;
    display: inline-block;
    margin-right: 14rpx;
}

.reserve-next-step {
    border-top: 1rpx solid #D7D7D7;
    padding: 34rpx 25rpx 20rpx;
}

.reserve-sku-title {
    font-size: 30rpx;
    color: #333333;
    line-height: 30rpx;
    display: inline-block;
    margin-right: 10rpx;
}

.next-step-btn {
    background: #FFC2AD;
    border-radius: 4rpx;
    font-size: 32rpx;
    color: #FFFFFF;
    line-height: 80rpx;
    height: 80rpx;
    text-align: center;
}

.next-step-btn.active {
    background: #FF6633;
    border-radius: 4rpx;
    color: #FFFFFF;
}