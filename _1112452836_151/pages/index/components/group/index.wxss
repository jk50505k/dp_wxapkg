.groups {
    margin-top: 36rpx;
}

.top {
    margin: 0 30rpx;
    height: 42rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.title {
    font-size: 30rpx;
    color: #777777;
    font-weight: 500;
}

.more,.distance {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.more,.distance,.other {
    font-size: 26rpx;
    color: #777777;
}

.icon {
    background-image: url(data:image/png;base64,);
    width: 9rpx;
    height: 18rpx;
    background-size: contain;
    background-repeat: no-repeat;
    margin-left: 10rpx;
}

.container {
    margin-top: 20rpx;
    height: 264rpx;
}

.group {
    position: relative;
    background: #FFFFFF;
    box-shadow: 0 2rpx 10rpx 0 rgba(0,0,0,0.05);
    border-radius: 10rpx;
    padding: 20rpx;
    width: 600rpx;
    overflow: hidden;
}

.group:before {
    content: '';
    border: solid #E1E1E1;
    border-width: 1px;
    border-radius: 20rpx;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.shop {
    position: relative;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 47rpx;
    line-height: 47rpx;
    padding-bottom: 13rpx;
    overflow: hidden;
}

.shop:before {
    content: '';
    border-bottom: solid #E1E1E1;
    border-width: 1px;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.shop-name {
    flex: 1;
    font-size: 26rpx;
    color: #111111;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-right: 12rpx;
    overflow: hidden;
}

.shop-icon {
    background-image: url(data:image/png;base64,);
    width: 32rpx;
    height: 32rpx;
    background-size: contain;
    background-repeat: no-repeat;
    margin-right: 10rpx;
}

.poi {
    flex: 1;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}

.desc {
    margin-top: 20rpx;
    display: flex;
    align-items: center;
}

.img {
    display: block;
    margin-right: 20rpx;
    width: 136rpx;
    height: 136rpx;
    background-image: url('data:image/png;base64,');
    background-size: contain;
    background-repeat: no-repeat;
}

.info {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-start;
    overflow: hidden;
}

.name {
    font-size: 32rpx;
    color: #333333;
    font-weight: 600;
    height: 45rpx;
    line-height: 45rpx;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}

.price,.other {
    margin-top: 10rpx;
}

.price {
    color: #FF6633;
    height: 34rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.basic {
    position: relative;
    padding: 6rpx 2rpx;
    margin-right: 16rpx;
    font-size: 22rpx;
    line-height: 24rpx;
    display: flex;
    align-items: center;
}

.basic:before {
    content: '';
    border: solid rgba(255,102,51,0.4);
    border-width: 1px;
    border-radius: 4rpx;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.number {
    position: relative;
    padding: 0 10rpx;
}

.discount {
    padding: 0 10rpx;
}

.next:before {
    content: '';
    border-right: solid rgb(255,102,51);
    border-width: 1px;
    transform: scale(0.5);
    transform-origin: 0 0;
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    pointer-events: none;
}

.now-price {
    font-size: 32rpx;
    color: #FF6633;
    font-weight: 500;
    line-height: 34rpx;
}

.other {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 37rpx;
}