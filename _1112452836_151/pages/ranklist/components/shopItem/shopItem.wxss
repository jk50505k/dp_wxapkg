.shoplist-item {
    box-sizing: border-box;
    background-color: #ffffff;
    margin: 0 30rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    margin-bottom: 10rpx;
    display: flex;
}

.hasBrandShop {
    margin-bottom: 0rpx!important;
    background-image: url('data:image/png;base64,');
    background-position: 0 top;
    background-repeat: repeat-x;
    background-size: auto 1px;
}

.parentShop,.lastChildShop {
    margin-bottom: 0rpx!important;
    padding-bottom: 0rpx!important;
}

.spreadParentShop {
    margin-bottom: 0rpx!important;
}

.shop-img-wrap {
    box-sizing: border-box;
    width: 190rpx;
    height: 190rpx;
    position: relative;
}

.special-shop {
    box-sizing: border-box;
    position: absolute;
    width: 100%;
    height: 100%;
    border: 4rpx solid #FCD67F;
    border-radius: 6rpx;
    background-color: transparent;
    z-index: 1;
}

.shop-ranktype {
    box-sizing: border-box;
    width: 98rpx;
    height: 36rpx;
    position: absolute;
    left: -4rpx;
    top: 14rpx;
    z-index: 2;
}

.shop-right-wrap {
    margin-left: 20rpx;
}

.shop-info-wrap-add-padding {
    padding-bottom: 20rpx;
}

.shop-info-notag .shop-name {
    margin-bottom: 38rpx;
}

.shop-info-notag .shop-sub {
    margin-bottom: 38rpx;
}

.shop-name {
    box-sizing: border-box;
    width: 480rpx;
    font-family: PingFangSC-Semibold;
    font-size: 32rpx;
    color: #333333;
    letter-spacing: 0;
    word-break: break-all;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    margin-bottom: 12rpx;
}

.shop-sub {
    display: flex;
    align-items: center;
    height: 37rpx;
    margin-bottom: 10rpx;
}

.star {
    display: inline-block;
    margin-right: 20rpx;
    width: 66px;
    height: 12px;
    vertical-align: middle;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 86px auto;
}

.star-0 {
    background-position: 0 -134px;
}

.star-10 {
    background-position: 0 -117px;
}

.star-20 {
    background-position: 0 -100px;
}

.star-30 {
    background-position: 0 -83px;
}

.star-35 {
    background-position: 0 -66px;
}

.star-40 {
    background-position: 0 -49px;
}

.star-45 {
    background-position: 0 -32px;
}

.star-50 {
    background-position: 0 -15px;
}

.shop-price {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #111111;
    text-align: left;
}

.shop-desc {
    display: flex;
    height: 37rpx;
    justify-content: space-between;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #777777;
    letter-spacing: 0;
    margin-bottom: 11rpx;
}

.shop-left {
    display: flex;
}

.shop-region {
    margin-right: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 280rpx;
}

.shop-type {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 280rpx;
}

.shop-tag {
    box-sizing: border-box;
    background: #FFEFEA;
    height: 38rpx;
    padding: 4rpx 12rpx;
    font-size: 22rpx;
    color: #FF6633;
}

.quan-wrap {
    background-image: url('data:image/png;base64,');
    background-position: 0 top;
    background-repeat: repeat-x;
    background-size: auto 1px;
    padding-top: 20rpx;
}

.add-minus-margin {
    margin-bottom: -10rpx!important;
}

.quan-item {
    margin-bottom: 16rpx;
    height: 32rpx;
    display: flex;
    align-items: center;
}

.delete-margin {
    margin-bottom: 0rpx!important;
}

.quan-icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}

.quan-content {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #777777;
    line-height: 26rpx;
    width: 432rpx;
    word-break: break-all;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}