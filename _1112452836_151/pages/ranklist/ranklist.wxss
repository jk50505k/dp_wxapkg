.page-error {
    display: block;
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 100%;
    z-index: 999;
    background-color: #ffffff;
    text-align: center;
    padding-top: 40%;
}

.error-img {
    width: 276rpx;
    height: 276rpx;
    display: block;
    margin: auto;
}

.error-word {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #777777;
    line-height: 32rpx;
    margin-bottom: 28rpx;
}

.search-btn {
    width: 236rpx;
    height: 70rpx;
    border: 1px solid #FF6633;
    border-radius: 42rpx;
    background-color: #ffffff;
    text-align: center;
    line-height: 70rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #FF6633;
    margin: auto;
}

.test-food {
    height: 294rpx;
    margin-bottom: 20rpx;
    background-color: #FF3F27;
}

.test-choosen {
    height: 354rpx;
    margin-bottom: 20rpx;
    background-color: #FFB76C;
}

.tabbar-wrap {
    padding: 0rpx 0rpx;
    margin: 0rpx 0rpx;
    box-sizing: border-box;
    width: 100%;
    background-color: #ffffff;
    position: relative;
}

.fixed-tabbar {
    position: fixed;
    top: 0;
    z-index: 10;
}

.tab-btn-wrap {
    box-sizing: border-box;
    width: 100%;
    padding: 0 40rpx;
    height: 96rpx;
    background-image: url('data:image/png;base64,');
    background-position: 0 bottom;
    background-repeat: repeat-x;
    background-size: auto 1px;
}

.hidden-all-city {
    height: 0rpx;
}

.all-city {
    display: flex;
    align-items: center;
    padding: 30rpx 30rpx 0rpx 30rpx;
}

.all-city-icon {
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-image: url('//p0.meituan.net/dpgroup/a3eb9c11effe66f224aa97e8f863cc191169.png');
    margin-right: 6rpx;
}

.all-city-title {
    font-family: PingFangSC-Semibold;
    font-size: 38rpx;
    color: #111111;
    line-height: 43rpx;
    font-weight: bold;
}

.tab-btn {
    box-sizing: border-box;
    width: 50%;
    height: 96rpx;
    display: inline-block;
    text-align: center;
    line-height: 96rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #111111;
    letter-spacing: 0;
    position: relative;
}

.active-tab {
    color: #ff6633;
    font-weight: bold;
}

.bottom-line {
    width: 335rpx;
    height: 6rpx;
    background-image: linear-gradient(-90deg,#FFB76C 0%,#FF3F27 100%);
    position: absolute;
    bottom: 0rpx;
}

.tag-btn-wrap {
    box-sizing: border-box;
    width: 100%;
    height: 96rpx;
    white-space: nowrap;
    padding-left: 30rpx;
    padding-top: 24rpx;
    padding-bottom: 19rpx;
    background-image: url('data:image/png;base64,');
    background-position: 0 bottom;
    background-repeat: repeat-x;
    background-size: auto 1px;
}

.tag-btn {
    box-sizing: border-box;
    display: inline-block;
    min-width: 105rpx;
    padding: 9rpx 26.5rpx;
    background-color: #f4f4f4;
    border-radius: 30rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #111111;
    text-align: center;
    margin-right: 10rpx;
}

.active-tag {
    background-color: #FFEFEA!important;
    color: #ff6633!important;
}

.last-tag {
    margin-right: 30rpx!important;
}

.shop-card {
    background-color: #ffffff;
}

.add-padding {
    padding-top: 192rpx;
}

.add-padding-small {
    padding-top: 96rpx!important;
}

.ghost {
    background-color: #ffffff;
}

.normal-ghost {
    height: 192rpx;
}

.small-ghost {
    height: 96rpx;
}

.list-loading {
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}

.end-wrap {
    height: 100rpx;
    line-height: 100rpx;
    color: #bbbbbb;
    text-align: center;
}

.loading-icon {
    vertical-align: middle;
    margin-right: 20rpx;
    width: 44rpx;
    height: 44rpx;
    display: inline-block;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: cover;
    transform-origin: center center;
    animation-name: loading;
    animation-duration: 1.2s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
}

@keyframes loading {
    0% {
        -webkit-transform: rotate(0);
    }

    50% {
        -webkit-transform: rotate(180deg);
    }

    to {
        -webkit-transform: rotate(1turn);
    }
}

@-webkit-keyframes loading {
    0% {
        -webkit-transform: rotate(0);
    }

    50% {
        -webkit-transform: rotate(180deg);
    }

    to {
        -webkit-transform: rotate(1turn);
    }
}

.shop-list-empty {
    position: relative;
    top: 60rpx;
    padding-bottom: 100rpx;
}

.empty-img {
    width: 276rpx;
    height: 276rpx;
    display: block;
    margin: 0 auto;
}

.empty-word {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #777777;
    line-height: 32rpx;
    text-align: center;
}