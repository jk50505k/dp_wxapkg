@import "__wuBaseWxss__/1.wxss";

page {
    font-size: 28rpx;
    height: 100%;
    width: 100%;
    background-color: #f6f6f6;
    font-family: -apple-system-font,'PingFang SC','Lantinghei SC','Source Han Sans','Microsoft YaHei','Helvetica Neue',Helvetica,sans-serif;
}

.entry {
    background-image: url(data:image/png;base64,);
    width: 15rpx;
    height: 26rpx;
    background-size: cover;
}

.page-error {
    padding-top: 130rpx;
    background-color: #f6f6f6;
    width: 100%;
    height: 100%;
    text-align: center;
    margin: 0 auto;
    color: #999;
}

.error-icon {
    width: 73rpx;
    height: 75rpx;
    background-image: url(data:image/png;base64,);
    background-size: cover;
    margin: 0 auto 80rpx;
}

.error-img {
    margin: 0 auto 80rpx;
    display: block;
    width: 200rpx;
}

.loadingImg {
    vertical-align: middle;
    margin-right: 5px;
    width: 44rpx;
    height: 44rpx;
    display: inline-block;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: cover;
    transform-origin: center center;
    animation-name: loading;
    animation-duration: 1.2s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
}

@keyframes loading {
    0% {
        -webkit-transform: rotate(0);
    }

    50% {
        -webkit-transform: rotate(180deg);
    }

    to {
        -webkit-transform: rotate(1turn);
    }
}

@-webkit-keyframes loading {
    0% {
        -webkit-transform: rotate(0);
    }

    50% {
        -webkit-transform: rotate(180deg);
    }

    to {
        -webkit-transform: rotate(1turn);
    }
}

.loadEnd {
    height: 50rpx;
}

.listContainer {
    background-color: #f6f6f6;
    min-height: 100%;
}

.emoji-img {
    position: relative;
    display: inline-block;
    width: auto;
    height: 18px;
    top: 1px;
    vertical-align: text-bottom;
}